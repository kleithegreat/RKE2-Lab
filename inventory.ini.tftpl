[rke2_servers]
%{ for server in servers ~}
${server.name} ansible_host=${server.network_interface[0].addresses[0]}
%{ endfor ~}

[rke2_agents]
%{ for agent in agents ~}
${agent.name} ansible_host=${agent.network_interface[0].addresses[0]}
%{ endfor ~}

[all:vars]
ansible_user=rocky
ansible_ssh_private_key_file=~/.ssh/id_rsa